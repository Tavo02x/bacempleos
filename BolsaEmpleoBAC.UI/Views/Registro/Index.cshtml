@*Paso #5 - Tu género*@
<table class="card-registro" id="paso5">
    <tr>
        <td class="td-progreso">
            @Html.Partial("_RegistroProgreso", 1)
        </td>
    </tr>
    <tr>
        <td class="td-contenido">
            <div class="mdl-cell mdl-cell--6-col">
                <p class="p-img-paso">
                    <img src="~/Content/Images/registro/registro-3.png" />
                </p>
                <h5>
                    Tu género es:
                </h5>
                <p>
                    <button class="mdl-button mdl-js-button mdl-js-ripple-effect btn-seleccionar-uno btn-genero" data-value="false" onclick="fnSeleccionarGenero(this)">
                        Femenino
                    </button>
                </p>
                <p>
                    <button class="mdl-button mdl-js-button mdl-js-ripple-effect btn-seleccionar-uno btn-genero" data-value="true" onclick="fnSeleccionarGenero(this)">
                        Masculino
                    </button>
                </p>
                <p>
                    <button class="mdl-button mdl-js-button mdl-js-ripple-effect btn-seleccionar-uno btn-genero" data-value="null" onclick="fnSeleccionarGenero(this)">
                        No Indica
                    </button>
                </p>
            </div>
        </td>
    </tr>
    <tr>
        <td class="td-footer">
            @Html.Partial("_RegistroBotonesFooter", 5)
        </td>
    </tr>
</table>

@*Paso #6 - Tu estado civil*@
<table class="card-registro" id="paso6">
    <tr>
        <td class="td-progreso">
            @Html.Partial("_RegistroProgreso", 1)
        </td>
    </tr>
    <tr>
        <td class="td-contenido">
            <div class="mdl-cell mdl-cell--6-col">
                <p class="p-img-paso">
                    <img src="~/Content/Images/registro/registro-4.png" />
                </p>
                <h5>
                    Tu estado civil es:
                </h5>
                <div class="mdl-grid">
                    <div class="mdl-cell mdl-cell--6-col">
                        <button class="mdl-button mdl-js-button mdl-js-ripple-effect btn-seleccionar-uno btn-estado-civil" id-estado-civil="1" onclick="fnSeleccionarEstadoCivil(this)">
                            Soltero
                        </button>
                    </div>
                    <div class="mdl-cell mdl-cell--6-col">
                        <button class="mdl-button mdl-js-button mdl-js-ripple-effect btn-seleccionar-uno btn-estado-civil" id-estado-civil="4" onclick="fnSeleccionarEstadoCivil(this)">
                            Casado
                        </button>
                    </div>
                    <div class="mdl-cell mdl-cell--6-col">
                        <button class="mdl-button mdl-js-button mdl-js-ripple-effect btn-seleccionar-uno btn-estado-civil" id-estado-civil="2" onclick="fnSeleccionarEstadoCivil(this)">
                            Divorciado
                        </button>
                    </div>
                    <div class="mdl-cell mdl-cell--6-col">
                        <button class="mdl-button mdl-js-button mdl-js-ripple-effect btn-seleccionar-uno btn-estado-civil" id-estado-civil="5" onclick="fnSeleccionarEstadoCivil(this)">
                            Únion libre
                        </button>
                    </div>
                    <div class="mdl-cell mdl-cell--6-col">
                        <button class="mdl-button mdl-js-button mdl-js-ripple-effect btn-seleccionar-uno btn-estado-civil" id-estado-civil="3" onclick="fnSeleccionarEstadoCivil(this)">
                            Viudo
                        </button>
                    </div>
                    <div class="mdl-cell mdl-cell--6-col">
                    </div>
                </div>
            </div>
        </td>
    </tr>
    <tr>
        <td class="td-footer">
            @Html.Partial("_RegistroBotonesFooter", 6)
        </td>
    </tr>
</table>

@*Paso #7 - Fecha de nacimiento*@
<table class="card-registro" id="paso7">
    <tr>
        <td class="td-progreso">
            @Html.Partial("_RegistroProgreso", 1)
        </td>
    </tr>
    <tr>
        <td class="td-contenido">
            <div class="mdl-cell mdl-cell--6-col">
                <p class="p-img-paso">
                    <img src="~/Content/Images/registro/registro-5.png" />
                </p>
                <h5>
                    Fecha de nacimiento:
                </h5>
            </div>
            <div class="mdl-cell mdl-cell--8-col">
                <div class="mdl-grid">
                    <div class="mdl-cell mdl-cell--2-col mdl-cell--3-col-phone">
                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                            <input class="mdl-textfield__input" type="number" id="txt-fecha-nacimiento-dia">
                            <label class="mdl-textfield__label" for="txt-fecha-nacimiento-dia">Día</label>
                        </div>
                    </div>
                    <div class="mdl-cell mdl-cell--4-col mdl-cell--3-col-phone">
                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                            <select class="mdl-textfield__input" id="slt-fecha-nacimiento-mes">
                                <option></option>
                                <option value="01">Enero</option>
                                <option value="02">Febrero</option>
                                <option value="03">Marzo</option>
                                <option value="04">Abril</option>
                                <option value="05">Mayo</option>
                                <option value="06">Junio</option>
                                <option value="07">Julio</option>
                                <option value="08">Agosto</option>
                                <option value="09">Setiembre</option>
                                <option value="10">Octubre</option>
                                <option value="11">Noviembre</option>
                                <option value="12">Diciembre</option>
                            </select>
                            <label class="mdl-textfield__label" for="slt-fecha-nacimiento-mes">Mes</label>
                        </div>
                    </div>
                    <div class="mdl-cell mdl-cell--2-col mdl-cell--3-col-phone">
                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                            <input class="mdl-textfield__input" type="number" id="txt-fecha-nacimiento-anno">
                            <label class="mdl-textfield__label" for="txt-fecha-nacimiento-anno">Año</label>
                        </div>
                    </div>
                </div>
            </div>
        </td>
    </tr>
    <tr>
        <td class="td-footer">
            @Html.Partial("_RegistroBotonesFooter", 7)
        </td>
    </tr>
</table>

